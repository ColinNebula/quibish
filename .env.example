# Environment-Specific Protection Settings

# Development Environment
NODE_ENV=development
REACT_APP_ENV=dev
REACT_APP_ENABLE_DEBUGGING=true
REACT_APP_INTEGRITY_CHECK=true

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_WEBSOCKET_URL=ws://localhost:5000

# Security Configuration
REACT_APP_ENABLE_PROTECTION=true
REACT_APP_CODE_SIGNING=true
REACT_APP_ANTI_TAMPER=true

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL=86400
BACKUP_RETENTION_DAYS=30
BACKUP_LOCATION=D:\Backups\quibish

# Monitoring
MONITORING_ENABLED=true
INTEGRITY_CHECK_INTERVAL=300000
ALERT_EMAIL=your-email@domain.com

# GitHub Configuration
GITHUB_OWNER=ColinNebula
GITHUB_REPO=quibish
GITHUB_BACKUP_REPO=quibish-backup

# Deployment Protection
DEPLOY_VERIFICATION=true
BUILD_SIGNATURE_CHECK=true
PRODUCTION_MINIFY=true
PRODUCTION_OBFUSCATE=true

# Legal/Compliance
COPYRIGHT_YEAR=2025
AUTHOR_NAME=ColinNebula
LICENSE_TYPE=MIT-PROTECTED
DMCA_PROTECTION=true

# Warning: Never commit production secrets to version control!
# Use separate .env.production file for production settings