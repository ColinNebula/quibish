{
  "name": "quibish",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://colinnebula.github.io/quibish",
  "dependencies": {
    "@apollo/client": "^4.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "form-data": "^4.0.4",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "reconnecting-websocket": "^4.4.0",
    "rxjs": "^7.8.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:memory": "node memory-start.js --frontend",
    "build": "react-scripts build",
    "build:no-sourcemap": "GENERATE_SOURCEMAP=false react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "smoke-check": "powershell -NoProfile -ExecutionPolicy Bypass -File ./tools/smoke-check.ps1",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run backend:dev\" \"npm run start\" --names \"API,APP\" --prefix-colors \"green,blue\"",
    "dev:memory": "concurrently \"npm run backend:memory\" \"npm run start:memory\" --names \"API,APP\" --prefix-colors \"green,blue\"",
    "backend:dev": "cd backend && nodemon server.js",
    "backend:start": "cd backend && node server.js",
    "backend:memory": "node memory-start.js --backend",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monitor": "pm2 monit",
    "pm2:status": "pm2 status",
    "startup": "pm2 startup",
    "save": "pm2 save"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "gh-pages": "^6.3.0",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10"
  }
}
