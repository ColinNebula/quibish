{
  "name": "quibish-backend",
  "version": "1.0.0",
  "description": "Backend server for Quibish messaging app",
  "main": "server.js",
  "scripts": {
    "start": "node stable-server.js",
    "start:stable": "node stable-server.js",
    "start:optimized": "node optimized-server.js",
    "start:simple": "node simple-server.js",
    "start:original": "node server.js",
    "start:managed": "powershell -File start-server.ps1 -ServerType stable -AutoRestart",
    "start:monitor": "powershell -File start-server.ps1 -ServerType stable -Monitor",
    "dev": "nodemon stable-server.js",
    "dev:optimized": "nodemon optimized-server.js",
    "health": "node -e \"require('http').get('http://localhost:5001/api/health', res => { let data = ''; res.on('data', chunk => data += chunk); res.on('end', () => console.log(JSON.parse(data))); })\"",
    "test": "npm run health"
  },
  "keywords": [
    "nodejs",
    "express",
    "quibish",
    "messaging",
    "api"
  ],
  "author": "Quibish Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "nodemailer": "^7.0.6",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
